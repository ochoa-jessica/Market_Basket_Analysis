   "source": [
    "# Apply the apriori algorithm\n",
    "frequent_itemsets = apriori(onehot, min_support=0.15, use_colnames=True, max_len=2)\n",
    "\n",
    "# Recover the association rules\n",
    "rules = association_rules(frequent_itemsets)\n",
    "\n",
    "rules.info()"
