  {
   "cell_type": "code",
   "execution_count": 27,
   "id": "4bfa1e02",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Conviction: 1.11\n"
     ]
