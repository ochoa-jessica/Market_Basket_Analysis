   "source": [
    "# Import apriori from mlxtend\n",
    "from mlxtend.frequent_patterns import apriori\n",
    "\n",
    "# Compute frequent itemsets using a support of 0.04 and length of 3\n",
    "frequent_itemsets_1 = apriori(onehot, min_support = 0.04, \n",
